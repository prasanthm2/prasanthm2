SELECT 
    TRIM(
        CASE 
            WHEN LOCATE('targettable ', cmd_tx) > 0 THEN
                SUBSTR(
                    cmd_tx, 
                    LOCATE('targettable ', cmd_tx) + LENGTH('targettable ') + 1, 
                    COALESCE(
                        LOCATE(' ', cmd_tx || ' ', LOCATE('targettable ', cmd_tx) + LENGTH('targettable ') + 1) 
                        - (LOCATE('targettable ', cmd_tx) + LENGTH('targettable ') + 1), 
                        LENGTH(cmd_tx)
                    )
                )
            ELSE ''
        END
    ) AS extracted_value
FROM 
    your_table;
